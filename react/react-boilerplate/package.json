{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "this is boiler plate for react project from scratch",
  "main": "src/index.js",
  "author": "lam99quyen@gmail.com",
  "license": "MIT",
  "scripts": {
    "prepare": "cd ../../ && husky install ./react/react-boilerplate/.husky",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config configs/webpack/webpack.dev.js",
    "build:clean": "rimraf ./build",
    "build": "yarn build:clean && cross-env NODE_ENV=production --config configs/webpack/webpack.prod.js --progress"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "eslint": "^8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
